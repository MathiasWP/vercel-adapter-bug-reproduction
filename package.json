{
	"name": "v2",
	"version": "0.0.1",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "svelte-kit dev --port 9000",
		"host": "svelte-kit dev --port 9000 --host",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"test": "npx playwright install && npx playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint:js": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"lint:css": "stylelint \"src/**/*.{css,scss,svelte}\"",
		"lint": "npm run lint:js && npm run lint:css",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "^1.0.0-next.51",
		"@sveltejs/kit": "1.0.0-next.330",
		"@types/cookie": "^0.4.1",
		"@types/luxon": "^2.0.9",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"@unocss/preset-wind": "^0.31.2",
		"autoprefixer": "^10.4.4",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-svelte3": "^3.2.1",
		"postcss": "^8.4.12",
		"postcss-html": "^1.3.0",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"sass": "^1.46.0",
		"stylelint": "^14.6.1",
		"stylelint-config-recommended-scss": "^6.0.0",
		"svelte": "^3.46.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.8.0",
		"tailwindcss": "^3.0.24",
		"tslib": "^2.3.1",
		"typescript": "^4.6.0"
	},
	"dependencies": {
		"luxon": "^2.3.0",
		"mrmime": "^1.0.0",
		"path-to-regexp": "^6.2.0",
		"socket.io-client": "^4.5.0",
		"xlsx": "^0.17.5"
	}
}
